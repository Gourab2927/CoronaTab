{
  "name": "@coronatab/data",
  "version": "1.0.0",
  "description": "CoronaTab data models and seeds",
  "main": "dist/index.js",
  "scripts": {
    "init:submodules": "git submodule update --init --recursive",
    "scrape": "cd coronadatascraper/ && source ~/.nvm/nvm.sh; nvm use 13.4; nvm current; npm i; NODE_OPTIONS='--insecure-http-parser' $(nvm which 13.4) cli.js",
    "generate-seeds": "./node_modules/.bin/ts-node -P ./tsconfig.json scripts/generate-seeds.ts",
    "sync": "./node_modules/.bin/ts-node scripts/sync.ts",
    "seed": "./node_modules/.bin/ts-node scripts/seed.ts",
    "build": "./node_modules/.bin/tsc -p ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coronatab/shared": "file:../shared",
    "@types/geojson": "^7946.0.7",
    "dasherize": "^2.0.0",
    "geojson": "^0.5.0",
    "pg": "^7.18.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@coronatab/node-utils": "file:../node-utils",
    "@types/csvtojson": "^1.1.5",
    "@types/node": "^13.9.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "moment": "^2.24.0"
  }
}
